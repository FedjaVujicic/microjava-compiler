program MJProgram

{

	int print_and_mul(char a, char b, int c, int d) {
		print(a);
		print(b);
		return c * d;
	}
	
	int square(int val) {
		return val * val;
	}
	
	int double(int val) {
		return 2 * val;
	}
	
	int double_and_square(int val) {
		return square(double(val));
	}
		
	void main() {
		print(print_and_mul('h', 'i', 20, 20));
		print(double_and_square(10));
	}

}